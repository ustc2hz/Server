<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.Lbs.model.ParkingBook" table="lbs_book" lazy="false">
		<id name="parkId" column="book_id" type="int">
			<generator class="native"></generator>	
		</id>
		<property name="managerId" column="manager_id" type="int" ></property>
		<property name="parkNum" column="book_number" type="int" ></property>
	</class>
</hibernate-mapping>