<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.Lbs.model.Admin" table="lbs_admin" lazy="false">
		<id name="adminId" column="admin_id" type="int">
			<generator class="native"></generator>	
		</id>
		<property name="adminName" column="admin_name" type="string" ></property>
		<property name="adminPassword" column="admin_password" type="string" ></property>
		<property name="parkPhone" column="admin_phone" type="string" ></property>
	</class>
</hibernate-mapping>